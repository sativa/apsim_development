------- manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- init
     in_crop_rain = 0
     eff_in_crop_rain = 0
     
     SECTION:- start_of_day
     if day = sow_day then
        soilwat2 reset
        soiln2 reset
        surfaceom reset
        fertiliz apply amount = nrate, type = no3_n, depth = 5 (mm)
        wheat sow cultivar = cultivar, plants = 100 (/m2), sowing_depth = 40 (mm)
        sowpaw = sw_dep(1) - ll_dep(1) + sw_dep(2) - ll_dep(2) + sw_dep(3) - ll_dep(3) + sw_dep(4) - ll_dep(4) + sw_dep(5) - ll_dep(5) + sw_dep(6) - ll_dep(6)
        sown = no3(1) + no3(2) + no3(3) + no3(4) + no3(5) + no3(6)
        sowing_paw2n = sowpaw / sown
        in_crop_rain = 0
        eff_in_crop_rain = 0
     endif
     if today = date('15-aug') then
         fertiliz apply amount = split_nrate, type = no3_n, depth = 5 (mm)
      endif
     
     SECTION:- end_of_day
     in_crop_rain = in_crop_rain + met.rain
     eff_in_crop_rain = eff_in_crop_rain + met.rain - soilwat2.drain - soilwat2.runoff
     if wheat.stage_name = 'harvest_ripe' or wheat.plant_status = 'dead' then
        hi = wheat.yield/wheat.biomass
        nhi = wheat.grain_n/wheat.biomass_n
        report do_output
        wheat harvest
        wheat end_crop
     endif
     
     SECTION:- init
     cultivar = 'hartog'
     
     SECTION:- init
     sow_day = 135
     
     SECTION:- init
     nrate = 40
     
     SECTION:- init
     split_nrate = 0
     END of rules
     
