[soil.soilwat2.parameters]
   diffus_const = 220    ! coeffs for unsaturated water flow
   diffus_slope = 22
   cn2_bare     = 60    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  100.000  100.000  150.000  200.000  200.000  300.000  400.000  400.000  400.000   ! layer thickness mm soil
   air_dry =    0.040    0.040    0.040    0.050    0.060    0.070    0.095    0.095    0.095   ! air dry mm water/mm soil
   ll15    =    0.040    0.040    0.040    0.050    0.060    0.070    0.095    0.095    0.095   ! lower limit mm water/mm soil
   dul     =    0.100    0.100    0.100    0.100    0.100    0.100    0.100    0.100    0.100   ! drained upper limit mm water/mm soil
   sat     =    0.370    0.340    0.340    0.340    0.340    0.340    0.340    0.340    0.340   ! saturation mm water/mm soil
   sw      =    0.040    0.060    0.062    0.055    0.060    0.070    0.095    0.095    0.095   ! starting soil water mm water/mm soil
   swcon   =    1.000    1.000    1.000    1.000    1.000    1.000    1.000    1.000    1.000   ! drainage coefficient
   bd      =    1.600    1.670    1.670    1.670    1.670    1.670    1.670    1.690    1.690   ! bulk density gm dry soil/cc moist soil

