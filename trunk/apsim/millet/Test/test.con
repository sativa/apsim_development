version = 6.1
[APSIM.test1_millet]
title=Test sowing before out
module = clock   test.par [all] %apsuite\apsim\clock\clock.xml [standard]
module = report     test.par[test1]  test.par[all]
module = input(met)        %apsuite/apsim/met/sample/dalby.met[weather]
module = manager    test.par[test1]   test.par[all]
module = soilwat2   c:\development/apsim/soilwat2/sample/soilwat2.par [black_earth] %apsuite\apsim\soilwat2\soilwat2.xml [standard]
module = SurfaceOM   c:\development/apsim/SurfaceOM/sample/SurfaceOM.par [legume_residue] %apsuite\apsim\SurfaceOM\SurfaceOM.xml [standard]
module = soiln2   c:\development/apsim/soiln2/sample/soiln2.par [black_earth] %apsuite\apsim\soiln2\soiln2.xml [standard]
module = canopy   test.par[all]
module = millet   test.par [all] %apsuite\apsim\millet\millet.xml [standard]
module = millet(millet1)   test.par [all] %apsuite\apsim\millet\millet1.xml [standard]
module = millet(millet2)   test.par [all] %apsuite\apsim\millet\millet2.xml [standard]
module = millet(millet3)   test.par [all] %apsuite\apsim\millet\millet3.xml [standard]
module = millet(millet4)   test.par [all] %apsuite\apsim\millet\millet4.xml [standard]
module = millet(millet5)   test.par [all] %apsuite\apsim\millet\millet5.xml [standard]

