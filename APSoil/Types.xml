      </drops>
   </folder>   
   <soil>
      <UItype>CSUserInterface.SoilUI</UItype>
      <ShowInMainTree>Yes</ShowInMainTree>
      <LargeIcon>%apsuite\apsimui\Images\shovel32.bmp</LargeIcon>
      <SmallIcon>%apsuite\apsimui\Images\shovel16.bmp</SmallIcon>
