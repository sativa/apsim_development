    <manager name="Calculator">
      <script>
        <event>init</event>
        <text> 
root_n = 0
head_n = 0
leaf_n = 0
sd = 0
tlai = 0
nConcGrainPC  = 0</text>
      </script>
      <script>
        <event>start_of_day</event>
        <text>
        </text>
      </script>
      <script>
        <event>end_of_day</event>
        <text><![CDATA[ 

root_n = n_green(1)
head_n = n_green(4)
leaf_n = n_green(2)

if (sw_demand > 0) then
   sd = sw_supply / sw_demand
else
   sd = 1
endif

if (sd > 1) then
   sd = 1
endif

tlai = lai + slai
nConcGrainPC = n_conc_grain * 100]]></text>
      </script>
