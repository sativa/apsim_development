version = 5.3
[apsim.test_array_size_100_soilwat2]

title=Soilwat2 test_array_size_100 Simulation
module = clock    soilwat2_test.par [test_array_size_100] %apsuite/apsim/clock/clock.ini[standard]
module = report   soilwat2_test.par [test_array_size_100]
module = input(Met)      %apsuite/apsim/met/sample/dalby.met[weather]
module = manager  soilwat2_test.par [test_array_size_100]
module = soilwat2 soilwat2_test.par [black_earth]     %apsuite/apsim/soilwat2/soilwat2.ini [standard]
module = SummaryFile soilwat2_test.par[test_array_size_100]

