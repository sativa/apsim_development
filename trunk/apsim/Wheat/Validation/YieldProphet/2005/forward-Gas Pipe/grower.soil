[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 60    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  100.000  100.000  150.000  200.000  200.000  300.000  400.000  400.000  650.000   ! layer thickness mm soil
   air_dry =    0.010    0.010    0.010    0.015    0.015    0.030    0.030    0.035    0.035   ! air dry mm water/mm soil
   ll15    =    0.020    0.020    0.030    0.030    0.030    0.060    0.060    0.070    0.070   ! lower limit mm water/mm soil
   dul     =    0.070    0.070    0.080    0.080    0.080    0.110    0.110    0.110    0.110   ! drained upper limit mm water/mm soil
   sat     =    0.430    0.410    0.410    0.420    0.440    0.380    0.400    0.400    0.380   ! saturation mm water/mm soil
   sw      =    0.052    0.069    0.069    0.067    0.070    0.086    0.083    0.090    0.070   ! starting soil water mm water/mm soil
   swcon   =    0.800    0.800    0.800    0.800    0.800    0.800    0.800    0.800    0.800   ! drainage coefficient
   bd      =    1.400    1.440    1.440    1.400    1.400    1.500    1.500    1.500    1.500   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
