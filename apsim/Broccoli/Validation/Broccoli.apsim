            <variable name="lai" description="Leaf area index" array="F" units="m^2/m^2" module="broccoli" variablename="lai" arrayspec=" " ModuleType="broccoli" />
            <variable name="floral_initiation_das" description="Days from sowing to floral initiation" array="F" units="days" module="broccoli" variablename="floral_initiation_das" arrayspec=" " ModuleType="broccoli" />
            <variable name="maturity_das" description="Days from sowing to maturity" array="F" units="days" module="broccoli" variablename="maturity_das" arrayspec=" " ModuleType="broccoli" />
            <variable name="stage" description="Plant stage" array="F" units="" module="broccoli" variablename="stage" arrayspec=" " ModuleType="broccoli" />
            <variable name="cum_vernal_days" description="Cumulative vernalisation" array="F" units="vd" module="broccoli" variablename="cum_vernal_days" arrayspec=" " ModuleType="broccoli" />
            <variable name="tt_tot" description="Thermal time spent in each crop stage" array="T" units="dd" module="broccoli" variablename="tt_tot" arrayspec=" " ModuleType="broccoli" />
            <variable name="dlt_cumvd" description="Todays vd" array="F" units="" module="broccoli" variablename="dlt_cumvd" arrayspec=" " ModuleType="broccoli" />
            <variable name="swdef_photo" description="Soil water deficit in photosynthesis" array="F" units="" module="broccoli" variablename="swdef_photo" arrayspec=" " ModuleType="broccoli" />
            <variable name="nfact_photo" description="N factor for photosynthesis" array="F" units="" module="broccoli" variablename="nfact_photo" arrayspec=" " ModuleType="broccoli" />
            <variable name="temp_stress_photo" description="Temperature Stress in photosynthesis" array="F" units="" module="broccoli" variablename="temp_stress_photo" arrayspec=" " ModuleType="broccoli" />
          </variables>
          <events>
            <event name="global.Daily" description="End of day" module="global" eventname="Daily" ModuleType="registrations" />
          </events>
        </outputfiledescription>
      </outputfile>
      <irrigation name="Irrigation">
        <property name="automatic_irrigation" type="text" description="switch">on</property>
        <property name="asw_depth" type="text" description="Depth to which ASW is calculated. (mm)">300.0</property>
        <property name="crit_fr_asw" type="text" description="Fraction of ASW below which irrigation is applied (0-1.0)">0.9</property>
        <property name="irrigation_efficiency" description="Efficiency of the irrigation. (0-1.0)">1.0</property>
      </irrigation>
      <broccoli />
      <outputfile name="final">
        <filename name="filename">GreenBelt1997 Planting1 final.out</filename>
        <outputfiledescription name="My Variables">
          <variables>
            <variable name="sowing_date" description="sowing date" array="F" units="days" module="broccoli" variablename="sowing_date" arrayspec=" " ModuleType="broccoli" />
            <variable name="dd/mm/yyyy" description="Date (dd/mm/yyyy)" module="global" variablename="dd/mm/yyyy" arrayspec=" " ModuleType="registrations" />
            <variable name="floral_initiation_das" description="Days from sowing to floral initiation" array="F" units="days" module="broccoli" variablename="floral_initiation_das" arrayspec=" " ModuleType="broccoli" />
            <variable name="maturity_das" description="Days from sowing to maturity" array="F" units="days" module="broccoli" variablename="maturity_das" arrayspec=" " ModuleType="broccoli" />
            <variable name="green_biomass_wt" description="Green Biomass weight" array="F" units="g/m^2" module="broccoli" variablename="green_biomass_wt" arrayspec=" " ModuleType="broccoli" />
            <variable name="pod_wt" description="Weight of pods" array="F" units="g/m^2" module="broccoli" variablename="pod_wt" arrayspec=" " ModuleType="broccoli" />
            <variable name="leaf_no" description="number of leaves per square meter" array="F" units="leaves/m2" module="broccoli" variablename="leaf_no" arrayspec=" " ModuleType="broccoli" />
          </variables>
