[soil.soilwat2.parameters]
   diffus_const = 40    ! coeffs for unsaturated water flow
   diffus_slope = 16
   cn2_bare     = 73    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  150.000  150.000  300.000  300.000  300.000  300.000   ! layer thickness mm soil
   air_dry =    0.080    0.100    0.130    0.160    0.180    0.180   ! air dry mm water/mm soil
   ll15    =    0.170    0.170    0.170    0.180    0.180    0.180   ! lower limit mm water/mm soil
   dul     =    0.350    0.380    0.410    0.360    0.360    0.390   ! drained upper limit mm water/mm soil
   sat     =    0.440    0.490    0.480    0.440    0.420    0.410   ! saturation mm water/mm soil
   sw      =    0.107    0.102    0.131    0.190    0.324    0.387   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.300    0.300    0.200    0.100    0.100   ! drainage coefficient
   bd      =    1.360    1.250    1.310    1.410    1.460    1.490   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.170      0.170      0.170      0.270      0.360      0.390
   kl      =      0.060      0.060      0.040      0.040      0.030      0.020
   xf      =      1.000      1.000      1.000      0.400      0.200      0.000

[soil.soiln2.parameters]
   root_cn      = 45     ! C:N ratio of initial root residues
   root_wt      = 1500   ! root residues as biomass (kg/ha)
   soil_cn      = 12.5   ! C:N ratio of soil
   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      1.480      0.800      0.300      0.100      0.100      0.100   ! Soil Organic Carbon
   ph      =      7.833      7.900      8.200      8.500      8.833      9.000   ! pH of soil
   fbiom   =      0.020      0.020      0.010      0.010      0.010      0.010   ! Organic C Biomass Fraction
   finert  =      0.400      0.600      0.800      0.930      0.950      0.950   ! Inert Organic C Fraction
   no3ppm  =      5.566      5.110      6.000      2.701      0.699      1.000   ! Nitrate Concentration
   nh4ppm  =      0.980      1.022      1.000      0.670      0.070      0.100   ! Ammonium Concentration

