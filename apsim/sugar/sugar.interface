   mint                        : APSIMReal              = Minimum air temperature, (oC), GET
   radn                        : APSIMReal              = Solar radiation, (Mj/m2), GET
   fr_intc_radn_nnnn           : APSIMReal              = Fraction of radiation intercepted by canopy (nnnn = module name), (), GET, 0.0, 1.0
   dlayer                      : APSIMRealArray         = Soil layer depths, (mm), GET
   dul_dep                     : APSIMRealArray         = Drained upper limit soil water content for soil layer, (mm), GET
   sw_dep                      : APSIMRealArray         = Soil water content of layer, (mm), GET
   sat_dep                     : APSIMRealArray         = Saturation limit soil water content for soil layer, (mm), GET
   ll15_dep                    : APSIMRealArray         = Lower limit 15 bar soil water content for soil layer, (mm), GET
   no3                         : APSIMRealArray         = Soil NO3 content, (kg/ha), GET
   NO3_min                     : APSIMRealArray         = Minimum soil NO3 content, (kg/ha), GET
   st                          : APSIMRealArray         = Soil temperature, (oC), GET
   
   dlt_NO3                     : APSIMDlt_NO3Method     = Change in NO3
   dlt_sw_dep                  : APSIMDlt_sw_depMethod  = Change in soil water  
   incorp_fom                  : APSIMIncorporateFOMMethod = Incorporate fresh organic matter

   Crop_Chopped                : APSIMCropChoppedEvent     = Crop chopped event
   
end

