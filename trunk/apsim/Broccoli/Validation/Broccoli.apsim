              <row_spacing type="text" description="Enter row spacing (mm) : ">600</row_spacing>
            </category>
          </rule>
          <rule name="Harvest on a Growth Stage">
            <condition name="start_of_day">
        if ([crop].stage_name = '[stage]') then
                    [crop] harvest
                    [crop] end_crop
        endif
               </condition>
            <category name="Harvesting criteria">
