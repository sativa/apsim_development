<simulations name="untitled" version="14">
  <simulation name="Fiesta1997">
    <clock name="clock">
      <start_date name="start_date" description="Select the start date of the simulation" type="date" lbound="1/01/1997" ubound="31/12/1998">1/03/1997</start_date>
      <end_date name="end_date" description="Select the end date of the simulation" type="date" lbound="1/01/1997" ubound="31/12/1998">10/11/1997</end_date>
    </clock>
    <summaryfile name="summaryfile">
      <filename name="filename">Fiesta1997.sum</filename>
    </summaryfile>
    <metfile name="met">
      <filename name="filename">C:\development\apsim\Broccoli\Validation\DPI9798.met</filename>
    </metfile>
    <area name="Planting1">
      <registrations name="global" />
      <folder name="Manager folder">
        <manager name="Sowing fertiliser">
          <script>
            <text>
        [fertmodule] apply amount = [fert_amount_sow] (kg/ha), depth = 50 (mm), type = [fert_type_sow]
            </text>
            <event>[modulename].[eventname]</event>
          </script>
          <ui>
            <category name="When should fertiliser be applied" />
            <modulename type="modulename" description="On which module should the event come from : ">broccoli</modulename>
            <eventname type="text" description="On which event should fertiliser be applied : ">sowing</eventname>
            <category name="Fertiliser application details" />
            <fertmodule type="modulename" description="Module used to apply the fertiliser : ">fertiliser</fertmodule>
            <fert_amount_sow type="text" description="Amount of starter fertiliser at sowing (kg/ha) : ">300</fert_amount_sow>
            <fert_type_sow type="list" listvalues="NO3_N, NH4_N, NH4NO3, urea_N, urea_no3, urea" description="Sowing fertiliser type : ">NO3_N</fert_type_sow>
          </ui>
        </manager>
        <manager name="Sow on a fixed date">
          <script>
            <text>
      if (today = date('[date]') then
                    [crop] sow plants =[density], sowing_depth = [depth], cultivar = [cultivar], row_spacing = [row_spacing]
        endif
               </text>
            <event>start_of_day</event>
          </script>
          <ui>
            <category name="Sowing criteria" />
            <date type="text" description="Enter sowing date (dd-mmm) : ">11-mar</date>
            <category name="Sowing parameters" />
            <crop type="modulename" description="Enter name of crop to sow : ">broccoli</crop>
            <density type="text" description="Enter sowing density  (plants/m2) : ">6</density>
            <depth type="text" description="Enter sowing depth  (mm) : ">50</depth>
            <cultivar type="cultivars" croppropertyname="crop" description="Enter cultivar : ">Fiesta</cultivar>
            <row_spacing type="text" description="Enter row spacing (mm) : ">600</row_spacing>
          </ui>
        </manager>
        <manager name="Harvest on a Growth Stage">
          <script>
            <text>
        if ([crop].StageName = '[stage]') then
                    [crop] harvest
                    [crop] end_crop
        endif
               </text>
            <event>start_of_day</event>
          </script>
          <ui>
            <category name="Harvesting criteria" />
            <stage type="text" description="Enter stage name : ">maturity</stage>
            <category name="Harvesting parameters" />
            <crop type="modulename" description="Enter name of crop to sow : ">broccoli</crop>
          </ui>
        </manager>
        <manager name="Irrigate at sowing">
          <script>
            <text><![CDATA[
         if ([irrig_amount_evt] > 0) then
            '[irrigatemodule]' apply amount = [irrig_amount_evt] (mm)
         endif
   ]]></text>
            <event>[modulename].[eventname]</event>
          </script>
          <ui>
            <category name="When should irrigation be applied" />
            <modulename type="modulename" description="The module the event come from : ">broccoli</modulename>
            <eventname type="text" description="Which event should irrigation be applied : ">sowing</eventname>
            <category name="Irrigation application details" />
            <irrigatemodule type="modulename" description="Module used to apply the irrigation : ">Irrigation</irrigatemodule>
            <irrig_amount_evt type="text" description="Amount of irrigation to apply (mm) : ">50</irrig_amount_evt>
          </ui>
        </manager>
      </folder>
      <fertiliser name="fertiliser" />
      <surfaceom name="surface organic matter">
        <PoolName description="Organic Matter pool name">wheat</PoolName>
        <type description="Organic Matter type">wheat</type>
        <mass description="Initial surface residue (kg/ha)">1000.</mass>
        <cnr description="C:N ration of initial residue">80.0</cnr>
        <standing_fraction description="Fraction of residue standing">0.0</standing_fraction>
      </surfaceom>
      <outputfile name="outputfile">
        <filename name="filename">Fiesta1997 Planting1.out</filename>
        <variables name="My Variables" shortcut="/untitled/shared/My Variables" />
        <events name="My Variables Events" shortcut="/untitled/shared/My Variables Events" />
      </outputfile>
      <broccoli />
      <outputfile name="final">
        <filename name="filename">Fiesta1997 Planting1 final.out</filename>
        <variables name="FinalVariables" shortcut="/untitled/shared/FinalVariables" />
        <events name="FinalVariables Events" shortcut="/untitled/shared/FinalVariables Events" />
      </outputfile>
      <irrigation name="Irrigation">
        <property name="automatic_irrigation" type="text" description="switch">on</property>
        <property name="asw_depth" type="text" description="Depth to which ASW is calculated. (mm)">600.0</property>
        <property name="crit_fr_asw" type="text" description="Fraction of ASW below which irrigation is applied (0-1.0)">0.5</property>
        <property name="irrigation_efficiency" description="Efficiency of the irrigation. (0-1.0)">1.0</property>
      </irrigation>
      <Soil shortcut="/untitled/shared/LawesSoil" name="LawesSoil">
        <InitWater shortcut="/untitled/shared/LawesSoil/InitWater" />
        <InitNitrogen shortcut="/untitled/shared/LawesSoil/InitNitrogen" />
      </Soil>
    </area>
