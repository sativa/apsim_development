   sat     =    0.370    0.320    0.280    0.280    0.260    0.270    0.380    0.370   ! saturation mm water/mm soil
   sw      =    0.030    0.034    0.063    0.127    0.167    0.251    0.286    0.336   ! starting soil water mm water/mm soil
   swcon   =    0.700    0.700    0.700    0.700    0.700    0.500    0.100    0.100   ! drainage coefficient
   bd      =    1.520    1.680    1.800    1.820    1.850    1.860    1.590    1.630   ! bulk density gm dry soil/cc moist soil

