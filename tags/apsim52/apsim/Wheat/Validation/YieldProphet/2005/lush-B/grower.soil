   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      0.992      0.357      0.218      0.110   ! Soil Organic Carbon
   ph      =      6.900      8.381      8.374      9.000   ! pH of soil
