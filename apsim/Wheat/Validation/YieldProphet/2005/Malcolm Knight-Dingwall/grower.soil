[soil.soilwat2.parameters]
   diffus_const = 40    ! coeffs for unsaturated water flow
   diffus_slope = 16
   cn2_bare     = 78    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  50.000  150.000  400.000  450.000  750.000   ! layer thickness mm soil
   air_dry =    0.100    0.120    0.198    0.220    0.220   ! air dry mm water/mm soil
   ll15    =    0.210    0.220    0.240    0.290    0.350   ! lower limit mm water/mm soil
   dul     =    0.400    0.400    0.410    0.400    0.390   ! drained upper limit mm water/mm soil
   sat     =    0.461    0.479    0.479    0.471    0.452   ! saturation mm water/mm soil
   sw      =    0.192    0.322    0.404    0.403    0.350   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.200    0.200    0.100    0.100   ! drainage coefficient
   bd      =    1.350    1.300    1.300    1.350    1.400   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.210      0.220      0.240      0.290      0.350
   kl      =      0.060      0.060      0.040      0.040      0.020
   xf      =      1.000      1.000      1.000      0.375      0.365

[soil.soiln2.parameters]
   root_cn      = 45     ! C:N ratio of initial root residues
