[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 60    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  100.000  100.000  150.000  200.000  200.000  300.000  400.000  400.000  650.000   ! layer thickness mm soil
   air_dry =    0.025    0.030    0.030    0.040    0.040    0.045    0.050    0.060    0.060   ! air dry mm water/mm soil
   ll15    =    0.050    0.060    0.065    0.080    0.080    0.090    0.100    0.120    0.120   ! lower limit mm water/mm soil
   dul     =    0.150    0.160    0.160    0.165    0.160    0.160    0.180    0.180    0.180   ! drained upper limit mm water/mm soil
   sat     =    0.450    0.420    0.450    0.390    0.390    0.390    0.350    0.450    0.450   ! saturation mm water/mm soil
   sw      =    0.138    0.147    0.137    0.111    0.090    0.093    0.136    0.140    0.120   ! starting soil water mm water/mm soil
   swcon   =    0.600    0.600    0.600    0.600    0.600    0.600    0.600    0.600    0.600   ! drainage coefficient
   bd      =    1.300    1.400    1.300    1.500    1.500    1.500    1.600    1.300    1.300   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.050      0.060      0.065      0.080      0.080      0.090      0.120      0.140      0.160
   kl      =      0.060      0.060      0.040      0.040      0.020      0.020      0.010      0.010      0.010
   xf      =      1.000      1.000      1.000      1.000      1.000      1.000      1.000      0.875      0.000

[soil.soiln2.parameters]
   root_cn      = 40     ! C:N ratio of initial root residues
   root_wt      = 1500   ! root residues as biomass (kg/ha)
   soil_cn      = 12.5   ! C:N ratio of soil
   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      0.610      0.700      0.390      0.100      0.010      0.010      0.010      0.010      0.010   ! Soil Organic Carbon
   ph      =      4.900      5.900      6.800      6.500      6.500      7.100      7.400      6.600      6.600   ! pH of soil
   fbiom   =      0.040      0.040      0.030      0.020      0.010      0.010      0.010      0.010      0.010   ! Organic C Biomass Fraction
   finert  =      0.100      0.100      0.100      0.200      0.200      0.800      0.800      0.900      0.900   ! Inert Organic C Fraction
   no3ppm  =      7.000      16.000      10.000      2.000      1.000      1.000      1.000      1.000      1.000   ! Nitrate Concentration
   nh4ppm  =      2.000      1.000      1.000      1.000      1.000      1.000      1.000      1.000      1.000   ! Ammonium Concentration

