[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 68    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  100.000  300.000  300.000  300.000  500.000  500.000   ! layer thickness mm soil
   air_dry =    0.060    0.080    0.095    0.129    0.170    0.170   ! air dry mm water/mm soil
   ll15    =    0.110    0.140    0.140    0.161    0.188    0.188   ! lower limit mm water/mm soil
   dul     =    0.210    0.260    0.260    0.310    0.370    0.370   ! drained upper limit mm water/mm soil
   sat     =    0.430    0.440    0.440    0.420    0.410    0.400   ! saturation mm water/mm soil
   sw      =    0.079    0.170    0.257    0.321    0.330    0.370   ! starting soil water mm water/mm soil
   swcon   =    0.400    0.400    0.300    0.300    0.200    0.100   ! drainage coefficient
   bd      =    1.400    1.400    1.400    1.450    1.500    1.550   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.110      0.140      0.180      0.240      0.330      0.370
   kl      =      0.060      0.050      0.040      0.040      0.020      0.020
   xf      =      1.000      1.000      1.000      0.400      0.000      0.000
[soil.canola.parameters]
   ll      =      0.110      0.140      0.200      0.280      0.370      0.370
   kl      =      0.080      0.080      0.070      0.050      0.050      0.040
   xf      =      1.000      1.000      1.000      0.400      0.000      0.000

[soil.soiln2.parameters]
   root_cn      = 45     ! C:N ratio of initial root residues
   root_wt      = 1500   ! root residues as biomass (kg/ha)
   soil_cn      = 12.5   ! C:N ratio of soil
   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      1.150      0.500      0.250      0.150      0.100      0.100   ! Soil Organic Carbon
   ph      =      7.000      7.700      8.800      9.100      9.100      8.800   ! pH of soil
   fbiom   =      0.025      0.019      0.011      0.010      0.010      0.010   ! Organic C Biomass Fraction
   finert  =      0.357      0.632      0.855      0.947      0.950      0.950   ! Inert Organic C Fraction
   no3ppm  =      32.000      8.000      2.000      2.000      1.000      1.000   ! Nitrate Concentration
   nh4ppm  =      2.009      1.220      0.508      0.217      0.100      0.100   ! Ammonium Concentration

