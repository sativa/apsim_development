   dul     =    0.320    0.320    0.330    0.350    0.350    0.340    0.330   ! drained upper limit mm water/mm soil
   sat     =    0.390    0.370    0.380    0.400    0.400    0.390    0.380   ! saturation mm water/mm soil
   sw      =    0.373    0.357    0.338    0.318    0.305    0.292    0.242   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.300    0.300    0.300    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.600    1.600    1.550    1.500    1.520    1.550    1.570   ! bulk density gm dry soil/cc moist soil

