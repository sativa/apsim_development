[soil.soilwat2.parameters]
   diffus_const = 140    ! coeffs for unsaturated water flow
   diffus_slope = 30
   cn2_bare     = 68    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  150.000  150.000  300.000  300.000  300.000   ! layer thickness mm soil
   air_dry =    0.015    0.038    0.057    0.080    0.070   ! air dry mm water/mm soil
   ll15    =    0.060    0.060    0.070    0.080    0.070   ! lower limit mm water/mm soil
   dul     =    0.150    0.150    0.170    0.190    0.190   ! drained upper limit mm water/mm soil
   sat     =    0.370    0.350    0.330    0.330    0.330   ! saturation mm water/mm soil
   sw      =    0.143    0.151    0.148    0.080    0.070   ! starting soil water mm water/mm soil
   swcon   =    0.700    0.600    0.600    0.600    0.600   ! drainage coefficient
   bd      =    1.590    1.630    1.680    1.700    1.700   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.060      0.060      0.070      0.080      0.070
   kl      =      0.060      0.055      0.040      0.040      0.030
   xf      =      1.000      1.000      1.000      1.000      1.000

[soil.soiln2.parameters]
   root_cn      = 45     ! C:N ratio of initial root residues
   root_wt      = 1500   ! root residues as biomass (kg/ha)
   soil_cn      = 12.5   ! C:N ratio of soil
   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      0.792      0.428      0.260      0.100      0.340   ! Soil Organic Carbon
   ph      =      5.800      4.733      5.100      5.600      5.900   ! pH of soil
   fbiom   =      0.025      0.019      0.013      0.010      0.010   ! Organic C Biomass Fraction
   finert  =      0.399      0.604      0.800      0.926      0.950   ! Inert Organic C Fraction
   no3ppm  =      11.367      7.301      6.000      2.775      1.000   ! Nitrate Concentration
   nh4ppm  =      1.004      0.996      1.000      0.277      0.100   ! Ammonium Concentration

