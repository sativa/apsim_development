[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 73    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  200.000  200.000  200.000  200.000  200.000   ! layer thickness mm soil
   air_dry =    0.091    0.149    0.167    0.205    0.285   ! air dry mm water/mm soil
   ll15    =    0.160    0.175    0.185    0.205    0.285   ! lower limit mm water/mm soil
   dul     =    0.280    0.290    0.290    0.290    0.290   ! drained upper limit mm water/mm soil
   sat     =    0.422    0.432    0.432    0.394    0.386   ! saturation mm water/mm soil
   sw      =    0.134    0.199    0.186    0.207    0.285   ! starting soil water mm water/mm soil
   swcon   =    0.400    0.300    0.200    0.100    0.100   ! drainage coefficient
   bd      =    1.400    1.400    1.400    1.500    1.600   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.160      0.175      0.185      0.205      0.285
   kl      =      0.060      0.050      0.040      0.040      0.040
   xf      =      1.000      1.000      0.334      0.215      0.109

[soil.soiln2.parameters]
   root_cn      = 45     ! C:N ratio of initial root residues
   root_wt      = 1500   ! root residues as biomass (kg/ha)
   soil_cn      = 12.5   ! C:N ratio of soil
   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      0.705      0.250      0.100      0.100      0.100   ! Soil Organic Carbon
   ph      =      8.300      8.500      8.900      9.900      9.400   ! pH of soil
   fbiom   =      0.024      0.017      0.012      0.009      0.010   ! Organic C Biomass Fraction
   finert  =      0.438      0.682      0.830      0.912      0.955   ! Inert Organic C Fraction
   no3ppm  =      11.000      5.000      3.000      3.201      1.985   ! Nitrate Concentration
   nh4ppm  =      1.000      1.000      1.000      0.320      0.198   ! Ammonium Concentration

