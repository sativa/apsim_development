   dul     =    0.100    0.100    0.100    0.120    0.130    0.150    0.160    0.160    0.160   ! drained upper limit mm water/mm soil
   sat     =    0.380    0.370    0.340    0.300    0.320    0.360    0.320    0.324    0.343   ! saturation mm water/mm soil
   sw      =    0.051    0.045    0.049    0.072    0.093    0.086    0.095    0.106    0.130   ! starting soil water mm water/mm soil
   swcon   =    0.800    0.800    0.800    0.800    0.800    0.800    0.800    0.800    0.800   ! drainage coefficient
   bd      =    1.500    1.500    1.640    1.760    1.720    1.600    1.700    1.690    1.630   ! bulk density gm dry soil/cc moist soil

