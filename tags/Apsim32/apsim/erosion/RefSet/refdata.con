! gmtb0[a-d] give each model driven by single/split equations under soilwat.
! gmtb0e gives eroding profile.
! gmtb0s gives swim/surface.

version = 3.2
[apsim.gmtb0a.control]

title=Greenmount B0_SOILWAT
module = clock refdata.par[gmtb0]
module = report refdata.par[gmtb0] refdata.par[gmtb0a]
module = input(met) GMTAPSIM.DAT[weather]
module = manager refdata.par[gmt]
module = soilwat2 refdata.par[gmtb0] %apsuite\apsim\soilwat2\soilwat2.ini[standard]
module = erosion refdata.par[gmtb0a]
module = residue2 refdata.par[gmtb0] %apsuite\apsim\residue2\residue2.ini[standard]
module = grasp refdata.par[gmtb0] %apsuite\apsim\grasp\grasp.ini[standard]
module = SummaryFile  refdata.par[gmtb0a]
module = Screen  refdata.par[gmtb0]
