Variable_alias = ca_uptk_eqv          mg_uptk_eqv         k_uptk_eqv          na_uptk_eqv

Module_names =   soilph             soilph             soilph      
Variable_names = sum@p_uptake_equiv sum@s_uptake_equiv sum@cl_uptake_equiv
Variable_alias = p_uptk_eqv         s_uptk_eqv         cl_uptk_eqv


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!  manager  !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
[sample.manager.init]    
   crop_in = 0
   first_day = 1

[sample.manager.start_of_day]    
   ! The date_within function returns 1 when todays date lies
   ! between the 2 dates inclusively.
   ! It must have its arguments enclosed in quotes.

   if (date_within('1-may, 1-aug') = 1 AND
             rain[5] >= 20 AND
             crop_in = 0 AND
             esw >= 30) THEN
      nwheat sow 100 30 Sunco
      crop_in = 1
      fertiliz apply amount = 100 (kg/ha), depth = 50 (mm), type = urea ()
   endif

   if (nwheat.nwheat_status = 10) then
      nwheat harvest
      crop_in = 0
      report do_end_day_output
      report1 do_end_day_output
   endif

[sample.manager.end_of_day]    
   if (first_day = 1) then
         ! report initial state
      report do_output
      report1 do_output
      first_day = 0
   endif

