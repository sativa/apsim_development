version = 6.1
[apsim.sample_sugar]


title = Sugar Test for capping of crop water use


module = clock   Test.par [sample] %apsuite/apsim/clock/clock.xml [standard]
module = report   Test.par [sample]
module = input(met)      %apsuite/apsim/met/sample/ingham.met [weather]
module = manager  Test.par [sample]
module = irrigate Test.par [sample]
module = fertiliz   %apsuite/apsim/fertiliz/fertiliz.xml [standard]
module = soilwat2   %apsuite/apsim/soilwat2/sample/soilwat2.par [black_earth] %apsuite/apsim/soilwat2/soilwat2.xml [standard]
module = SurfaceOM   %apsuite/apsim/SurfaceOM/sample/SurfaceOM.par [sugar_residue] %apsuite/apsim/SurfaceOM/SurfaceOM.xml [standard]
module = soiln2   %apsuite/apsim/soiln2/sample/soiln2.par [black_earth] %apsuite/apsim/soiln2/soiln2.xml [standard]
module = sugar   Test.par [sample] %apsuite/apsim/sugar/sugar.xml [standard]


