   sat     =    0.479    0.476    0.468    0.461    0.457    0.449    0.442   ! saturation mm water/mm soil
   sw      =    0.352    0.396    0.392    0.383    0.368    0.401    0.340   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.300    0.300    0.300    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.300    1.310    1.330    1.350    1.360    1.380    1.400   ! bulk density gm dry soil/cc moist soil

