<simulation name="Raider6" executable="%apsuite\apsim\protocolmanager\lib\protocolmanager.dll">
  <title>Raider6</title>
  <component name="clock" executable="%apsuite\apsim\clock\lib\clock.dll">
    <executable>%apsuite\apsim\clock\lib\clock.dll</executable>
    <initdata>
      <include>%apsuite\apsim\clock\clock.ini</include>
      <start_date name="start_date" description="Select the start date of the simulation" type="date" lbound="1/01/2000" ubound="15/10/2007">12-jul-2001</start_date>
      <end_date name="end_date" description="Select the end date of the simulation" type="date" lbound="1/01/2000" ubound="15/10/2007">15/10/2007</end_date>
    </initdata>
  </component>
  <component name="met" executable="%apsuite\apsim\input\lib\input.dll">
    <executable>%apsuite\apsim\input\lib\input.dll</executable>
    <initdata>
      <filename name="filename">C:\development\apsim\Lettuce\Validation\Phenology\Gatton.met</filename>
    </initdata>
  </component>
  <system name="Paddock" executable="%apsuite\apsim\protocolmanager\lib\protocolmanager.dll">
    <component name="Outputfile" executable="%apsuite\apsim\report\lib\report.dll">
      <executable>%apsuite\apsim\report\lib\report.dll</executable>
      <initdata>
        <outputfrequency>harvesting</outputfrequency>
        <variable>day</variable>
        <variable>sowing_date</variable>
        <variable>sowing_das</variable>
        <variable>maturity_das</variable>
        <variable>maturity_date</variable>
      </initdata>
    </component>
    <component name="accum" executable="%apsuite\apsim\accum\lib\accum.dll">
      <initdata>
        <accum_variables>rain[15]</accum_variables>
      </initdata>
    </component>
    <component name="Manager" executable="%apsuite\apsim\manager\lib\manager.dll">
      <executable>%apsuite\apsim\manager\lib\manager.dll</executable>
      <initdata>
        <rule name="Sow on a fixed date - start_of_day" condition="start_of_day">
                    
      if (today = date('12-jul-2001') then
                    lettuce sow plants =6, sowing_depth = 50, cultivar = Raider, row_spacing = 600
        endif
               
                    </rule>
        <rule name="Harvest on a Growth Stage - start_of_day" condition="start_of_day">
                    
        if (lettuce.stage_name = 'maturity') then
                    lettuce harvest
                    lettuce end_crop
        endif
               
                    </rule>
      </initdata>
    </component>
    <component name="Soil Water" executable="%apsuite\apsim\soilwat2\lib\soilwat2.dll">
      <initdata>
        <include>%apsuite\apsim\soilwat2\soilwat2.ini</include>
        <diffus_const>40</diffus_const>
        <diffus_slope>16</diffus_slope>
        <cn2_bare>73</cn2_bare>
        <cn_red>20</cn_red>
        <cn_cov>0.8</cn_cov>
        <salb>0.12</salb>
        <SummerCona>3.5</SummerCona>
        <WinterCona>2.5</WinterCona>
        <SummerU>6</SummerU>
        <WinterU>4</WinterU>
        <SummerDate>1-nov</SummerDate>
        <WinterDate>1-apr</WinterDate>
        <dlayer> 150 150 300 300 300 300 300  </dlayer>
        <sat>0.570 0.530 0.520 0.520 0.500 0.470 0.460 </sat>
        <dul>0.520 0.480 0.470 0.470 0.450 0.420 0.410 </dul>
        <ll15>0.250 0.250 0.250 0.250 0.250 0.250 0.250 </ll15>
        <air_dry>0.130 0.200 0.250 0.250 0.250 0.250 0.250 </air_dry>
        <swcon>0.3 0.3 0.3 0.3 0.3 0.3 0.3 </swcon>
        <bd>1.05 1.16 1.19 1.18 1.24 1.31 1.35 </bd>
        <sw>    0.520    0.480    0.470    0.470    0.450    0.420    0.410</sw>
      </initdata>
    </component>
    <component name="Soil Nitrogen" executable="%apsuite\apsim\soiln2\lib\soiln2.dll">
      <initdata>
        <include>%apsuite\apsim\soiln2\soiln2.ini</include>
        <root_cn>40</root_cn>
        <root_wt>1000</root_wt>
        <soil_cn>12</soil_cn>
        <enr_a_coeff>7.4</enr_a_coeff>
        <enr_b_coeff>0.2</enr_b_coeff>
        <profile_reduction>off</profile_reduction>
        <oc>      1.190      1.010      0.450      0.300      0.200      0.160      0.170</oc>
        <ph>      8.200      8.500      8.600      8.600      7.400      5.300      5.300</ph>
        <fbiom> 0.04 0.02 0.02 0.02 0.01 0.01 0.01</fbiom>
        <finert> 0.4 0.6 0.8 1 1 1 1</finert>
        <ureappm>0  0    0    0    0    0    0</ureappm>
        <no3ppm>      0.100      0.100      0.100      0.100      0.100      0.100      0.100</no3ppm>
        <nh4ppm>      0.100      0.100      0.100      0.100      0.100      0.100      0.100</nh4ppm>
      </initdata>
    </component>
    <component name="lettuce" executable="%apsuite\apsim\plant\lib\plant.dll">
      <executable>%apsuite\apsim\plant\lib\plant.dll</executable>
      <initdata>
        <include>%apsuite\apsim\lettuce\lettuce.ini</include>
        <ll>    0.250    0.250    0.250    0.250    0.250    0.250    0.250</ll>
        <kl>      0.060      0.060      0.060      0.060      0.060      0.060      0.060</kl>
        <xf>      1.000      1.000      1.000      1.000      1.000      1.000      1.000</xf>
      </initdata>
    </component>
