------- Manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- start_of_day
           if (paddock_is_fallow() = 1 and fallowin <> 'yes' and (nextcrop = 0 or nextcrop = 'centro')) then
              if (date_within('23-sep, 23-sep') = 1) then
     if (rain[3] >= 30 and esw >= 200) or
         ('yes' = 'yes' and today = date('23-sep'))) then
        choosenextcrop = 'yes'
        centro sow plants =20, sowing_depth = 30, cultivar = cavalcade, row_spacing = 250, crop_class = plant
     endif
     if today = date('23-sep') then
        choosenextcrop = 'yes'
     endif
              endif
           endif
     
     SECTION:- end_of_day
                if centro.stage_name = 'harvest_ripe' or centro.plant_status = 'dead' then
       centro  harvest
       centro  end_crop
                endif
     
     SECTION:- centro.sowing
             fertiliser apply amount = 0 (kg/ha), depth = 50 (mm), type = urea_n
     END of rules
     

