[sample.report.parameters]
title = soybean Sample Simulation
screen_output = on
outputfile  = soybean.out /overwrite
summaryfile = soybean.sum /overwrite

module_names=    clock clock
variable_names=  day   year     
variable_alias=  -     -      

module_names=    soybean soybean soybean
variable_names=  lai      biomass  green_biomass
variable_alias=  -        -        -     
units=           -         -       -     

[sample.clock.parameters]
! Start and end date of run (day number of year and year)
start_day =  320
start_year = 1988
end_day =  90
end_year = 1989

[sample.manager.start_of_day]

if day = 330 and year  = 1988 then
   soybean sow crop_type = soybean, cultivar = davis, plants = 25 (/m2), sowing_depth = 40 (mm)

elseif day = 80 and year = 1989 then
   soybean harvest
   soybean end_crop   

endif

[sample.manager.end_of_day]
report do_output


[sample.soybean.parameters]

   ll    = .230 .240 .240 .250 .260 .270 .280  ! lower limit mm water/mm soil
   xf   = 1.00 1.00 1.00 1.00 1.00 1.00 1.00  ! Weighting factor for root advance
   kl   = 0.08 0.08 0.08 0.08 0.05 0.03 0.01  ! Water Extraction Parameter (0-1)