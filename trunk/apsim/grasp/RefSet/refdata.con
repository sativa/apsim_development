! Derived from McKeon's 7 steps. 
! See http://apsru.sth.dpi.qld.gov.au/~devoilp/grasp for a detailed explanation

! All off. 
[APSIM.step1.control]
module=report   [all] [step1]
module=input    [all]              narayen.met[weather]
module=met
module=manager  [all]
module=soilwat2 [all]              soilwat2.ini[standard]
module=grasp    [all] [step1] [soilwat]     grasp.ini[standard]
module=residue2 [all]              residue2.ini[standard]

! Introduce dead grass
[APSIM.step3.control]
module=report   [all] [step3]
module=input    [all]              narayen.met[weather]
module=met
module=manager  [all]
module=soilwat2 [all]              soilwat2.ini[standard]
module=grasp    [all] [step3] [soilwat]     grasp.ini[standard]
module=residue2 [all]              residue2.ini[standard]

! Add growth by regrowth
[APSIM.step5.control]
module=report   [all] [step5]
module=input    [all]              narayen.met[weather]
module=met
module=manager  [all]
module=soilwat2 [all]              soilwat2.ini[standard]
module=grasp    [all] [step5] [soilwat]     grasp.ini[standard]
module=residue2 [all]              residue2.ini[standard]

! And growth by transpiration
[APSIM.step6.control]
module=report   [all] [step6]
module=input    [all]              narayen.met[weather]
module=met
module=manager  [all]
module=soilwat2 [all]              soilwat2.ini[standard]
module=grasp    [all] [step6] [soilwat]     grasp.ini[standard]
module=residue2 [all]              residue2.ini[standard]

! SWIM interface.
[APSIM.step8.control]
module=report   [gmtb0]
module=input    [gmtb0]             gmtapsim.dat[weather]
module=met
module=manager  [all]
module=apswim   [gmtb0]             c:\apsuite\apsim\apswim\apswim.ini[standard] 
module=grasp    [all] [step8]  [swim]    grasp.ini[standard]
module=residue2 [gmtb0]             residue2.ini[standard]
module=surface  [gmtb0]

! EP effect on Nitrogen uptake
[APSIM.ep-on.control]
module=report   [ep]  [ep-on]
module=input    [ep]  narayen.met[weather]
module=met
module=soilwat2 [all]  soilwat2.ini[standard]
module=erosion  [ep] 
module=grasp    [all] [ep] [soilwat] grasp.ini[standard]
module=residue2 [all]  residue2.ini[standard]
module=manager  [ep]

[APSIM.ep-off.control]
module=report   [ep]  [ep-off]
module=input    [ep]  narayen.met[weather]
module=met
module=soilwat2 [all]  soilwat2.ini[standard]
module=grasp    [all] [ep] [soilwat] grasp.ini[standard]
module=residue2 [all]  residue2.ini[standard]
module=manager  [ep]


[all.report.parameters]
screen_output = off

module_names=  met   
variable_names=today.dd/mm/yyyy 
variable_alias=date    
units=         -    
!                
module_names=    
variable_names=  
variable_alias=  
units=           

!              [reset_sw.dlt_sw_dep], [soil_moisture.sw_dep] [soil_evap.basal_area]
module_names=  soilwat2               soilwat2               grasp 
variable_names=sw_dep()               sw_dep                 basal_area       
variable_alias=-                      -                      -        
units=         -                      -                      -        

!               [profile.root_depth] [dul.sw_index]           [pan.ep]      [vpd.vpd_estimation]
!                                    [soil_moisture.sw_index]
module_names =  grasp                grasp                     manager       grasp
variable_names =root_depth           sw_index                  transpiration vpd_estimation
variable_alias= -                    -                         -             -
units=          -                    -                         -             -

!               [erosion.max_n_avail]     [canopy.growth_photo] [temperature.death_frost]
!               [bulk density.max_n_avail][radiation.growth_photo]
module_names =  grasp                      grasp                 grasp
variable_names =max_n_avail()              growth_photo          death_frost
variable_alias= -                          -                     -
units=          -                          -                     -

!              [add_residue_msg.residue_wt]
module_names  =residue2  
variable_names=residue_wt
variable_alias=-       
units=         -       

[gmtb0.report.parameters]
outputfile = step8.out  /overwrite
summaryfile = step8.sum /overwrite
title = Step 8. swim

screen_output = on

module_names=  met   
variable_names=today.dd/mm/yyyy 
variable_alias=date    
units=         -    

!               [water_uptake.uptake_water_grass]
module_names =  manager
variable_names =transpiration
variable_alias= -
units=          -

[ep.report.parameters]
screen_output = off

module_names=  met   
variable_names=today.dd/mm/yyyy 
variable_alias=date    
units=         -    

!                [erosion.n_uptake]
module_names =   grasp
variable_names = max_n_avail()
variable_alias = -
units =          -

[ep-on.report.parameters]
