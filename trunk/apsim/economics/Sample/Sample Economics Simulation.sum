------- Manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- start_of_day
     if (available('tractor=t1,implement=p1')) then
           if (paddock_is_fallow() = 1 and fallowin <> 'yes' and (nextcrop = 0 or nextcrop = 'wheat')) then
              if (date_within('15-may, 10-jul') = 1) then
     if (rain[3] >= 30 and
         esw >= 200 ) <> 0) or
         ('no' = 'yes' and today = date('10-jul'))) then
        choosenextcrop = 'yes'
        economics operate tractor=t1, implement = p1, area = 200
        economics expenditure category = seedcost, name = wheat, rate = 120 (kg/ha), area = 200 (ha)
        wheat sow plants =100, sowing_depth = 30, cultivar = hartog, row_spacing = 250, crop_class = plant
     endif
     if today = date('10-jul') then
        choosenextcrop = 'yes'
     endif
              endif
           endif
     endif
     
     SECTION:- end_of_day
     if wheat.stage_name = 'harvest_ripe' or wheat.plant_status = 'dead' then
       economics expenditure category = contract, name = broadacre_harvesting, rate = 1, area = 200 (ha)
       cropyieldtonnes = wheat.yield / 1000.0
       if ('wheat' = 'wheat') then
           protein = wheat.grain_protein
           economics income category= cropprice, name = wheat, yield = cropyieldtonnes, protein = protein, area = 200 (ha)
       else
           economics income category= cropprice, name = wheat, yield = cropyieldtonnes, area = 200 (ha)
       endif
       wheat  harvest
       wheat  end_crop
     endif
     
     SECTION:- wheat.sowing
             economics expenditure category = fertilisercost, name =  urea, rate = 150, area = 200 (ha)
             fertiliser apply amount = 150 (kg/ha), depth = 50 (mm), type =  urea
     END of rules
     

