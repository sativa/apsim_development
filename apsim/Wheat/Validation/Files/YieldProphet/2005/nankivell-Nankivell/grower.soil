   dul     =    0.190    0.210    0.240    0.156    0.024    0.290    0.290   ! drained upper limit mm water/mm soil
   sat     =    0.412    0.374    0.384    0.250    0.038    0.318    0.318   ! saturation mm water/mm soil
   sw      =    0.070    0.080    0.105    0.083    0.011    0.280    0.290   ! starting soil water mm water/mm soil
   swcon   =    0.600    0.500    0.500    0.500    0.200    0.200    0.200   ! drainage coefficient
   bd      =    1.400    1.500    1.500    1.605    1.770    1.700    1.700   ! bulk density gm dry soil/cc moist soil

