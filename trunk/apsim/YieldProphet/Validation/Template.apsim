          <script name="post">
            <text><![CDATA[   ! -------------------------------------------------------------
   ! Fire an event if we're in a window from
   ! now + 10 days.
   ! -------------------------------------------------------------
   if (today = TodayDate) then
      StartTenDayWindow = today 
      EndTenDayWindow = today + 10      
   endif
   if (today >= StartTenDayWindow and today < EndTenDayWindow)  then
      rain = 0
      Daily do_end_day_output
   endif
]]></text>
            <event>post</event>
          </script>
