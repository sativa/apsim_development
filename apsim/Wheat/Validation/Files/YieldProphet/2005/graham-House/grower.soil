   dul     =    0.300    0.315    0.330    0.330    0.330    0.336    0.336   ! drained upper limit mm water/mm soil
   sat     =    0.384    0.366    0.385    0.385    0.395    0.376    0.376   ! saturation mm water/mm soil
   sw      =    0.150    0.320    0.326    0.310    0.310    0.320    0.336   ! starting soil water mm water/mm soil
   swcon   =    0.400    0.400    0.400    0.400    0.300    0.400    0.200   ! drainage coefficient
   bd      =    1.500    1.600    1.550    1.550    1.550    1.600    1.600   ! bulk density gm dry soil/cc moist soil

