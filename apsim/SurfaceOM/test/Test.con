version = 5.3
[apsim.Test1]

title= Surface Organic Matter Module Test 1
module = clock    Test.par [sample] %apsuite/apsim/clock/clock.ini[standard]
module = report   Test.par [test1]
module = input(Met)      optimum.MET [weather]
module = manager  Test.par [test1]
module = soilwat2 %apsuite/apsim/soilwat2/sample/soilwat2.par [black_earth]     %apsuite/apsim/soilwat2/soilwat2.ini [standard]
module = SurfaceOM  Test.par [test1]     %apsuite/apsim/SurfaceOM/SurfaceOM.ini [standard]
module = soiln2   %apsuite/apsim/soiln2/sample/soiln2.par [black_earth]     %apsuite/apsim/soiln2/soiln2.ini [standard]
module = Log Test.par[test1]

