module = manager manager.par [modify_met]
module = summaryfile manager.par [modify_met]
module = log manager.par[test]

[test_big_module_names]

title=Manager Sample Simulation
module = clock   manager.par [sample] %apsuite\apsim\clock\clock.ini[standard]
