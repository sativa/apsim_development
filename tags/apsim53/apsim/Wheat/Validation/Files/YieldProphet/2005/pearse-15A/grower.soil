[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 60    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  100.000  100.000  150.000  200.000  200.000  300.000  400.000  400.000  1000.000   ! layer thickness mm soil
   air_dry =    0.020    0.020    0.020    0.020    0.020    0.020    0.020    0.020    0.020   ! air dry mm water/mm soil
   ll15    =    0.040    0.040    0.040    0.040    0.040    0.040    0.040    0.040    0.040   ! lower limit mm water/mm soil
   dul     =    0.080    0.080    0.080    0.080    0.080    0.080    0.080    0.090    0.090   ! drained upper limit mm water/mm soil
   sat     =    0.370    0.340    0.340    0.340    0.340    0.340    0.340    0.340    0.340   ! saturation mm water/mm soil
   sw      =    0.045    0.048    0.048    0.061    0.063    0.048    0.051    0.048    0.040   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.300    0.300    0.300    0.300    0.300    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.600    1.700    1.700    1.700    1.700    1.700    1.700    1.700    1.700   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.040      0.040      0.040      0.040      0.040      0.040      0.040      0.040      0.040
   kl      =      0.080      0.080      0.080      0.060      0.060      0.040      0.020      0.010      0.010
