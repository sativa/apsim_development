[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 78    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  100.000  100.000  200.000  200.000  300.000  300.000  300.000   ! layer thickness mm soil
   air_dry =    0.055    0.065    0.084    0.145    0.162    0.170    0.170   ! air dry mm water/mm soil
   ll15    =    0.110    0.100    0.120    0.170    0.170    0.170    0.170   ! lower limit mm water/mm soil
   dul     =    0.240    0.220    0.240    0.340    0.340    0.340    0.340   ! drained upper limit mm water/mm soil
   sat     =    0.375    0.385    0.404    0.404    0.395    0.395    0.395   ! saturation mm water/mm soil
   sw      =    0.253    0.161    0.170    0.170    0.198    0.220    0.290   ! starting soil water mm water/mm soil
   swcon   =    0.600    0.500    0.500    0.400    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.550    1.550    1.500    1.500    1.550    1.550    1.550   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.120      0.100      0.120      0.170      0.180      0.220      0.290
   kl      =      0.060      0.060      0.040      0.040      0.030      0.020      0.020
   xf      =      1.000      1.000      1.000      1.000      1.000      1.000      1.000

[soil.soiln2.parameters]
   root_cn      = 45     ! C:N ratio of initial root residues
   root_wt      = 1500   ! root residues as biomass (kg/ha)
   soil_cn      = 12.5   ! C:N ratio of soil
   enr_a_coeff  = 7.4
   enr_b_coeff  = 0.2
   profile_reduction =  off

   oc      =      1.600      0.800      0.400      0.200      0.100      0.100      0.100   ! Soil Organic Carbon
   ph      =      6.000      4.700      5.400      5.400      6.000      9.000      9.000   ! pH of soil
   fbiom   =      0.025      0.022      0.017      0.012      0.010      0.010      0.010   ! Organic C Biomass Fraction
   finert  =      0.357      0.510      0.682      0.830      0.926      0.950      0.950   ! Inert Organic C Fraction
   no3ppm  =      29.000      15.000      4.000      4.000      2.000      1.000      1.000   ! Nitrate Concentration
   nh4ppm  =      3.000      3.000      3.000      3.000      1.000      0.100      0.100   ! Ammonium Concentration

