   dul     =    0.100    0.320    0.320    0.320    0.320    0.310    0.306   ! drained upper limit mm water/mm soil
   sat     =    0.384    0.356    0.366    0.404    0.404    0.376    0.338   ! saturation mm water/mm soil
   sw      =    0.090    0.272    0.320    0.278    0.270    0.288    0.306   ! starting soil water mm water/mm soil
   swcon   =    0.700    0.400    0.400    0.400    0.300    0.300    0.200   ! drainage coefficient
   bd      =    1.500    1.600    1.600    1.500    1.500    1.600    1.700   ! bulk density gm dry soil/cc moist soil

