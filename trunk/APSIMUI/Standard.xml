        <property name="tillage_depth" value="0" description="User_defined depth of seedbed preparation (mm) : " />
        <property name="tillage_f_incorp" value="0" description="User_defined fraction of surface residues to incorporate (0-1): " />
      </category>
    </rule>
    <rule name="Reset water, nitrogen and surfaceOM on fixed date">
      <condition name="start_of_day">
         if (today = date('[reset_date]') then
            resetWater = '[resetWater]'
            resetNitrogen  = '[resetNitrogen]'
            resetSurfaceOM = '[resetSurfaceOM]'
            if (resetWater = 'yes') then
