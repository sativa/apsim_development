------- manager Initialisation ------------------------------------------------
     Manager rules:
     if day = 10 and year = 1988 then
         maize sow cultivar = dekalb_xl82, plants = 10, sowing_depth = 30 (mm)
     endif
     if day = 10 and year = 1988 then
         cowpea sow cultivar = banjo, plants = 25 (/m2), sowing_depth = 40 (mm)
     endif
     if (maize.stage = 11) then
        maize harvest
        maize end_crop
     endif
     if ( maize.stage >=  1 and
          maize.stage <= 11 and
          maize.plant_status = 'dead') then
          maize  end_crop
     endif
     if cowpea.stage_name = 'harvest_ripe' or cowpea.plant_status = 'dead' then
        cowpea harvest
        cowpea end_crop
     endif
     if ((maize.stage >=1 and maize.stage <= 11) or
         (cowpea.stage >= 1 and cowpea.stage <= 11 )) then
         report do_output
     endif

