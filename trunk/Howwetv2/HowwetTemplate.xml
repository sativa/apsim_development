<simulations name="HowWet" version="5">
  <simulation name="HowWet">
    <clock name="clock">
      <start_date name="start_date" description="Select the start date of the simulation" type="date">
      </start_date>
      <end_date name="end_date" description="Select the end date of the simulation" type="date">
      </end_date>
    </clock>
    <summaryfile name="summaryfile">
      <filename name="filename">
      </filename>
    </summaryfile>
    <metfile name="met">
      <filename name="filename">
      </filename>
    </metfile>
    <area name="paddock">
      <registrations name="global" />
      <surfaceom name="surface organic matter">
        <PoolName description="Organic Matter pool name">wheat</PoolName>
        <type description="Organic Matter type">wheat</type>
        <mass description="Initial surface residue (kg/ha)">1000.</mass>
        <cnr description="C:N ratio of initial residue">80.0</cnr>
        <standing_fraction description="Fraction of residue standing">0.0</standing_fraction>
      </surfaceom>
      <outputfile name="outputfile">
        <variables name="My Variables">
          <variable name="sw as SoilWater" description="Soil water - layered (mm/mm)" array="?" />
          <variable name="day as Day" description="Day" array="?" />
          <variable name="rain as Rainfall" description="Rainfall (mm)" array="?" />
          <variable name="surface organic matter.surfaceom_wt as SurfaceOrganicMatter" description="Total mass of all surface organic materials" array="?" />
          <variable name="runoff as Runoff" description="Runoff (mm)" array="?" />
          <variable name="es as Evapoation" description="Evaporation (mm)" array="?" />
          <variable name="sw(1) as SoilWaterTopLayer" description="Soil water - layered (mm/mm)" array="?" />
          <variable name="no3() as NO3Total" description="NO3 - layered (kg/ha)" array="?" />
          <variable name="maxt as MaxTemp" description="Maximum temperature (oC)" array="?" />
          <variable name="year as Year" description="Year" array="?" />
          <variable name="Erosion.soil_loss as SoilLoss" description="Soil loss (t/ha)" array="?" />
          <variable name="drain as Drain" description="Drainage (mm)" array="?" />
        </variables>
        <events name="My Variables Events">
          <event name="Daily" description="End of day" array="No" />
        </events>
      </outputfile>
      <erosion name="Erosion">
        <model name="model">freebairn</model>
        <slope name="slope">1.0</slope>
        <slope_length name="slope_length">60.0</slope_length>
        <k_factor name="k_factor">0.4</k_factor>
        <p_factor name="p_factor">1.0</p_factor>
        <bed_depth name="bed_depth">3000</bed_depth>
        <profile_reduction name="profile_reduction">off</profile_reduction>
        <profile_layer_merge name="profile_layer_merge">0.05</profile_layer_merge>
        <minimum_depth name="minimum_depth">100.0</minimum_depth>
      </erosion>
    </area>
  </simulation>
</simulations>