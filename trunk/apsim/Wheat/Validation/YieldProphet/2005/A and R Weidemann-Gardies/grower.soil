   sat     =    0.527    0.527    0.527    0.541    0.541    0.522    0.518    0.516   ! saturation mm water/mm soil
   sw      =    0.133    0.258    0.258    0.272    0.272    0.283    0.337    0.390   ! starting soil water mm water/mm soil
   swcon   =    0.700    0.300    0.300    0.300    0.300    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.174    1.174    1.174    1.136    1.136    1.187    1.198    1.204   ! bulk density gm dry soil/cc moist soil

