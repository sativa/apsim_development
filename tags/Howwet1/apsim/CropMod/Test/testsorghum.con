version = 5.2
[APSIM.test1_Sorghum]
title=Test sowing before out
module = clock      testsorghum.par[all] %apsuite/apsim/clock/clock.ini[standard]
module = report     testsorghum.par[test1]  testsorghum.par[all]
module = input(met)        %apsuite/apsim/met/sample/dalby.met[weather]
module = manager    testsorghum.par[test1]   testsorghum.par[all]
module = soilwat2   %apsuite/apsim/soilwat2/sample/soilwat2.par[black_earth]  %apsuite/apsim/soilwat2/soilwat2.ini[standard]
module = SurfaceOM   %apsuite/apsim/SurfaceOM/sample/SurfaceOM.par [legume_residue] %apsuite/apsim/SurfaceOM/SurfaceOM.ini [standard]
module = soiln2     %apsuite/apsim/soiln2/sample/soiln2.par[black_earth]      %apsuite/apsim/soiln2/soiln2.ini[standard]
module = sorghum  testsorghum.par[all] %apsuite/apsim/sorghum/sorghum.ini [standard]
module = SummaryFile  testsorghum.par[test1]

