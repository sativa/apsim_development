include base.interface

; -----------------------------------------------------------
; Sugar's public interface which other components may
; use.  Contains:
;    Properties which other components may get or set
;    Methods which other components may call
;    Events which other components may trap (or subscribe to)
; -----------------------------------------------------------
class APSIMSugarSowMethod
   plants             : APSIMReal           = Plant density, (plants/m2mm), 0.0, 100.0
   ratoon             : APSIMInteger        = Ratoon number, (), 0, 10
   sowing_depth       : APSIMReal           = Sowing depth, (mm), 0.0, 500.0
   cultivar           : APSIMString         = Name of cultivar, ()
end

class APSIMSugarHarvestMethod : APSIMMethod
    type                : APSIMString    = Type of Crop, (), GET
end

class APSIMSugarEndCropMethod : APSIMMethod
    type                : APSIMString    = Type of Crop, (), GET
end

class APSIMSugarKillCropMethod : APSIMMethod
    type                : APSIMString    = Type of Crop, ()
end

class APSIMSugarGrazeMethod : APSIMMethod
   fraction            : APSIMReal      = Fraction removed, (), 0.0, 1.0
   n_eff               : APSIMReal      = Fraction of N returned to the soil, (), 0.0, 1.0
   c_eff               : APSIMReal      = Fraction of C returned to the soil, (), 0.0, 1.0
   report              : APSIMString    = Report flag (optional), ()
end

class APSIMSugarHillupMethod : APSIMMethod
   topsfr              : APSIMReal      = Fraction of tops covered, (), 0.0, 1.0
   canefr              : APSIMReal      = Fraction of cane conered, (), 0.0, 1.0
end

class APSIMSugarLodgeMethod : APSIMMethod
   type                : APSIMString    = Type of Crop, ()
end

class APSIMDlt_NO3Method
   dlt_NO3             : APSIMRealArray       = Soil NO3 change, (kg/ha)
end

class APSIMDlt_sw_depMethod
   dlt_sw_dep             : APSIMRealArray       = Soil NO3 change, (kg/ha)
end

