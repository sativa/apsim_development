     proc pawc {} {
       set sw [apsimGet sw_dep]
       set ll [apsimGet ll15_dep]
       set pawc 0.0
       for {set i 0} {$i < [llength $sw]} {incr i} {
           set pawc [expr $pawc + ([lindex $sw $i] - [lindex $ll $i])]
       }
       return $pawc
