       set sw  [lindex [apsimGet sw_dep] [expr $layer-1]]
       set ll  [lindex [apsimGet ll15_dep] [expr $layer-1]]
       set fesw [expr ($sw - $ll)/($dul - $ll)]
       return $fesw
