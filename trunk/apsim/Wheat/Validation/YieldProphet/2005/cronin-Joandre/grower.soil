[soil.soilwat2.parameters]
   diffus_const = 40    ! coeffs for unsaturated water flow
   diffus_slope = 16
   cn2_bare     = 73    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  150.000  150.000  300.000  300.000  300.000  300.000   ! layer thickness mm soil
   air_dry =    0.100    0.160    0.180    0.200    0.200    0.200   ! air dry mm water/mm soil
   ll15    =    0.200    0.200    0.200    0.200    0.200    0.200   ! lower limit mm water/mm soil
   dul     =    0.410    0.400    0.390    0.390    0.380    0.380   ! drained upper limit mm water/mm soil
   sat     =    0.488    0.479    0.461    0.442    0.433    0.414   ! saturation mm water/mm soil
   sw      =    0.210    0.244    0.234    0.211    0.261    0.370   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.300    0.300    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.250    1.300    1.350    1.400    1.450    1.500   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.200      0.200      0.220      0.240      0.290      0.370
   kl      =      0.060      0.055      0.040      0.040      0.030      0.020
