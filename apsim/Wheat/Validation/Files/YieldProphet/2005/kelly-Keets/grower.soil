   sat     =    0.380    0.380    0.360    0.340    0.330    0.370    0.370    0.360    0.310   ! saturation mm water/mm soil
   sw      =    0.049    0.054    0.056    0.071    0.066    0.076    0.109    0.112    0.114   ! starting soil water mm water/mm soil
   swcon   =    0.300    0.300    0.300    0.300    0.300    0.300    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.300    1.540    1.600    1.640    1.680    1.580    1.580    1.580    1.580   ! bulk density gm dry soil/cc moist soil

