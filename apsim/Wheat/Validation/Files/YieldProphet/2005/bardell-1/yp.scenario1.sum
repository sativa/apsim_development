------- manager Initialisation ------------------------------------------------
     Manager rules:
     
     SECTION:- init
        moisturecontent = 12
        population = 100
        gm = 0.0
        costs = 0.0
        num_n_applications = 0
        base_price = 0
        min_protein = 0
        protein_incr = 0
        n_cost = 0
        n_appl_cost = 0
     
     SECTION:- start_of_day
        phase = 'none'
        if (today = date('19-jun')) then
           surfaceom tillage type = planter, f_incorp = 0.1 (), tillage_depth = 50
           wheat sow cultivar = clf-jnz, plants = population (plants/m2), sowing_depth = 30
           phase = 'sowing'
           sow_year = year
           zadok30das = 0
           zadok60das = 0
           zadok70das = 0
           zadok90das = 0
           report do_end_day_output
        endif
        harvesttoday = 'no'
        if (wheat.plant_status = 'dead' or wheat.stage_name = 'harvest_ripe') then
           harvesttoday = 'yes'
        endif
        if (harvesttoday = 'yes') then
           wet_yield = wheat.yield / (1 - moisturecontent/100) / 1000
           cropprotein = wheat.grain_protein
           if (cropprotein > 14) then
              cropprotein = 14
           endif
           price = base_price + (cropprotein - min_protein) * 2 * protein_incr
           costs = n_cost * napplied + n_appl_cost * num_n_applications
           gm = price * wet_yield - costs
           phase = 'harvest'
           report do_output
           if (today > date('01/04/2005')) then
              summary do_output
           endif
           wheat harvest
           wheat kill_crop
           wheat end_crop
           surfaceom tillage type = burn
        endif
        if (today = date('19-jun')
            or (today = date('19-jun') and date('19/06/2005') < date('19/06/2005'))) then
           soilwat2 reset
           soiln2 reset
           surfaceom reset
        endif
        if day > 90 and day < 273 then
           soilwat2.cona = 2.0
           soilwat2.u = 2.0
        else
           soilwat2.cona = 3.5
           soilwat2.u = 4.0
        endif
        if (today = date('01/04/2005')) then
           act_mods startseason
        endif
        if (today > date('01/04/2005')) then
           if (today = simulation_end_date) then
              phase = 'today'
              if (das > 0) then
                 summary do_end_day_output
              endif
           endif
        endif
        if (today >= date('19/06/2005')) then
           thisyear do_end_day_output
        endif
        if (zadok_stage >= 30 and zadok30das = 0) then
           zadok30das = das
        elseif (zadok_stage >= 60 and zadok60das = 0) then
           zadok60das = das
        elseif (zadok_stage >= 70 and zadok70das = 0) then
           zadok70das = das
        elseif (zadok_stage >= 90 and zadok90das = 0) then
           zadok90das = das
        endif
        if (today = date('19-jun')) then
           fertiliz apply amount = 8 (kg/ha), depth = 20 (mm), type = no3_n
        endif
     
     SECTION:- end_of_day
        water = esw_layer() + esw_layr()
     END of rules
     
