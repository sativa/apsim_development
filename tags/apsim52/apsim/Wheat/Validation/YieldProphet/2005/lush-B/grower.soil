[soil.soilwat2.parameters]
   diffus_const = 88    ! coeffs for unsaturated water flow
   diffus_slope = 35
   cn2_bare     = 78    ! bare soil runoff curve number
   cn_red       = 20    ! potetial reduction in curve number due to residue
   cn_cov       = 0.8   ! cover for maximum reduction in curve number
   salb         = 0.13  ! bare soil albedo
   cona         = 2   ! stage 2 evap coef.
   u            = 2     ! stage 1 soil evaporation coefficient (mm)

   dlayer  =  80.000  270.000  380.000  270.000   ! layer thickness mm soil
   air_dry =    0.065    0.188    0.219    0.220   ! air dry mm water/mm soil
   ll15    =    0.130    0.250    0.240    0.290   ! lower limit mm water/mm soil
   dul     =    0.280    0.375    0.344    0.306   ! drained upper limit mm water/mm soil
   sat     =    0.374    0.425    0.394    0.356   ! saturation mm water/mm soil
   sw      =    0.149    0.254    0.271    0.290   ! starting soil water mm water/mm soil
   swcon   =    0.600    0.300    0.300    0.300   ! drainage coefficient
   bd      =    1.500    1.443    1.527    1.627   ! bulk density gm dry soil/cc moist soil

[soil.wheat.parameters]
   ll      =      0.130      0.250      0.240      0.290
   kl      =      0.060      0.055      0.040      0.040
   xf      =      1.000      1.000      0.017      0.224

[soil.soiln2.parameters]
